pragma solidity >=0.7.0 <0.9.0;

contract test {
    struct Person{
        string name;
        uint age;
        string email;
    }

    Person public person;

    function setPerson(string memory name, uint age, string memory email) public{
        person = Person(name, age, email);
    }
}
